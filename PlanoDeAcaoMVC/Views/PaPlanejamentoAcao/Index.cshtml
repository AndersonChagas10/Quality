            /*validação*/

            /*Dados*/
            var param1 =  $('#newItem').val();
            var param2 = tempName.split('_Id')[0];
            var param3 = 0;
            if ($('#encadeado').find('select').length) {
                param3 = $('#encadeado').find('select').val();
            }

            /*Ajax*/
            $.get(urlSalvarNovoItem + "/" + param1 + "/" + param2 + "/" + param3, {}, function (r) {
                $(tempInput).append('<option value=' + r + '> ' + $('#newItem').val() + '</option>');
                $(tempInput).val(r).change();
                $('#modalNovo').modal('hide');
                tempInput = undefined;
                tempName = undefined;
                /*Limpa caso haja DDL denrto do modal.*/
                if ($('#encadeado').find('select').length) {
                    $('#encadeado').find('select').remove();
                }
            });

        }
