        var jaLiberouCallback = false;
        function SalvarAcaoAvulsa(o, isEdit) {

            var sendObj = $(o).serializeObject();
            if (isEdit == undefined) {
                sendObj['Panejamento_Id'] = planejamentoCorrentId;
            }
            sendObj['AcaoXQuem'] = [];

            $('.quemClone').each(function (c, o) {
                if ($('#AcaoXQuemDdlId').length)
                    sendObj['AcaoXQuem'].push({ Id: $(o).find('#AcaoXQuemDdlId').val(), Quem_Id: $(o).find('select').val() });
                else if ($(o).find('select').val())
                    sendObj['AcaoXQuem'].push({ Quem_Id: $(o).find('select').val() });

            });

            $.post(urlSalvarAcao, sendObj, function (r) {
                $.post(urlGetPlanejamentoAcao, {}, function (r) {

                    if (counter == 1 && !jaLiberouCallback) {
                        jaLiberouCallback = true;
                        $('#modalLindo').modal('hide');
                        alert('Registro salvo com sucesso!');
                    }
                    GerarTabela(r);

                }).fail(function (e, h, x) {

                    alert('Não foi possível salvar o registro: ' + e);

                }).always(function () {

                });
            });

