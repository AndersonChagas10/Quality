                                                 }
                                                 finally {
                                                     $('#formEditTatico #modalLindo select').select2();
                                                 }

                                             });
                                         });
                                         $('#formEditTatico #IndicadoresDeProjeto_Id').change();

                                     });
                                 });
                                 //HideForOperacional();
                             }
                        );
                    }

                    if (data.Acao.Id > 0) {

                        $.get(urlEditAcao, { id: data.Acao.Id },
                              function (r) {
