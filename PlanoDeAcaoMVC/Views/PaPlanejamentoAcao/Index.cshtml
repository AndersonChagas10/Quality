<h2>Plano de Ação</h2>

<div>
    <button type="button" class="btn btn-primary" data-toggle="modal" id="" onclick="Clicked(false, false); ModalOpcoesEstrategico('Planejamento Estratégico'); ">
        Planejamento Estratégico
    </button>
    <button type="button" class="btn btn-primary" data-toggle="modal" id="" onclick="Clicked(true, false); ModalOpcoesEstrategico('Planejamento Tático'); ">
        Planejamento Tático
    </button>
    <button type="button" class="btn btn-primary" data-toggle="modal" id="" onclick="Clicked(false, true); ModalOpcoesEstrategico('Planejamento Operacional'); ">
        Planejamento Operacional
    </button>
    <button type="button" onclick="Relatorios();" class="btn btn-primary">
        Relatórios
    </button>
</div>

<hr />

<div>
    <table id="paTable" class="table-striped dt-body-justify dt-body-nowrap"></table>
</div>

<div class="container">
    <div class="modal fade" id="modalLindo" tabindex="-1" role="dialog" aria-labelledby="" data-backdrop="static">
        <div class="modal-dialog" style="max-width:685px;" role="document">
            <div class="modal-content">
                <div class="modal-header">

                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true" class="times">&times;</span></button>
                    <button class="close modalMinimize"><span aria-hidden="true" class="minus">&minus;</span></button>


                    <h4 class="modal-title" id="Header">ID: Header</h4>
                </div>
                <div class="modal-body" id="body">
                    ID: Body
                </div>
                <div class="modal-footer">

                    <button type="button" class="btn btn-default" id="Fechar" onclick="Clicked(false, false);" data-dismiss="modal" style="display:none">Fechar</button>
                    <button type="button" class="btn btn-default" id="Fechar2" onclick="" style="display:none">Fechar</button>
                    <button type="button" class="btn btn-primary" id="Selecionar" onclick="" style="display:none" disabled>Selecionar</button>
                    <button type="button" class="btn btn-primary" id="Salvar" onclick="" style="display:none">Salvar</button>
                    <button type="button" class="btn btn-info" id="BuscarPlanejamentos" onclick="BuscarPlanejamentos();">Buscar Planejamento</button>
                    <button type="button" class="btn btn-success" id="NovoPlanejamento" onclick="NovoPlanejamento();" data-toggle="modal" style="display:none"> Novo Planejamento</button>
                    <button type="button" class="btn btn-primary" id="NovaAcao" onclick="Clicked(isTaticoClicked, isNovaAcao); ModalAcao(true, true);" style="display:none">Nova Ação</button>
                    <button type="button" class="btn btn-default" value="" id="new" style="display:none">Adicionar Ação</button>

                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal fade in" id="modalNovo" tabindex="-1" role="dialog" aria-labelledby="">
    <div class="modal-dialog" id="" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true" class="times">×</span></button>
                <button type="button" class="close modalMinimize" aria-label="Minimizar"><span aria-hidden="true" class="minus">−</span></button>
                <h4 class="modal-title" id="novoHeader"></h4>
            </div>
            <div class="modal-body" id="novoBody">
                <form>
                    <div class="form-group">
                        <input type="text" class="form-control" id="newItem">
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Fechar</button>
                <button type="button" class="btn btn-primary" onclick="SalvarNovo();">Salvar</button>

            </div>
        </div>
    </div>
</div>

<div class="minmaxCon"></div>

<script src="~/Scripts/angular.min.js"></script>

@section Scripts {

    <script>

        var GETGrupoCausa = '@Url.Action("GETGrupoCausa", "Pa_Acao")';
        var GETContramedidaGenerica = '@Url.Action("GETContramedidaGenerica", "Pa_Acao")';
        var planejamentoCorrentId = 0;
        var isTaticoClicked = false;
        var isNovaAcao = false;
        var urlModalOpcoesEstrategico = '@Url.Action("Details", "Pa_Planejamento")';
        var urlNovoPlanejamento = '@Url.Action("Index", "Pa_Planejamento")';
        var urlBuscarPlanejamentos = '@Url.Action("Buscar", "Pa_Planejamento")';
        var urlFiltraPlanejamentos = '@Url.Action("Filtrar", "Pa_Planejamento")';
        var urlSalvarPlanejamento = '@Url.Action("Save", "api/Pa_Planejamento")';
        var urlNovaAcao = '@Url.Action("Index", "Pa_Acao")';
        var urlEditAcao = '@Url.Action("Edit", "Pa_Acao")';
        var urlSalvarAcao = '@Url.Action("Save", "api/Pa_Acao")';
        var urlGetPlanejamentoAcao = '@Url.Action("GetPlanejamentoAcao", "api/Pa_Planejamento")';
        var urlSalvarNovoItem = '@Url.Action("Save", "api/Generics")';

        var addButton = (function () {
            var counter = 0;

            return function () {
                var objectContainer = $('#obj');
                var header = objectContainer.clone();
                counter += 1;
                var collapseId = "collapsible_obj_" + counter;

                header.find(".collapse").attr("id", collapseId);
                header.find("button[data-toggle='collapse']").attr("data-target", "#" + collapseId);
                header.find("button[data-toggle='collapse']").html("Acao Name" + counter)

                header.appendTo('#final');
                header.show();

                myfunction();
            };

        })();
        $('#new').click(addButton);

        /*Btn Minimizar Modal*/
        $(document).ready(function () {

            var $content, $modal, $apnData, $modalCon;
            $content = $(".min");

            //To fire modal
            $(".mdlFire").click(function (e) {
                e.preventDefault();
                var $id = $(this).attr("data-target");
                $($id).modal({ backdrop: false, keyboard: false });
            });


            $(".modalMinimize").on("click", function () {

                $modalCon = $(this).closest(".modal").attr("id");

                $apnData = $(this).closest(".modal");

                $modal = "#" + $modalCon;

                $(".modal-backdrop").addClass("display-none");

                $($modal).toggleClass("min");

                if ($($modal).hasClass("min")) {
                    $(".minmaxCon").append($apnData);
                    ///$(this).find("i").toggleClass('fa-minus').toggleClass('fa-clone');
                } else {
                    $("body").append($apnData);
                    ///$(this).find("i").toggleClass('fa-clone').toggleClass('fa-minus');
                };
            });

            $("button[data-dismiss='modal']").click(function () {
                $(this).closest(".modal").removeClass("min");
                $(".container").removeClass($apnData);
                //$(this).next('.modalMinimize').find("i").removeClass('fa fa-clone').addClass( 'fa fa-minus');
            });

        });
        /*FIM Btn Minimizar Modal*/

        $.fn.serializeObject = function () {
            var o = {};
            var a = this.serializeArray();
            $.each(a, function () {
                if (o[this.name] !== undefined) {
                    if (!o[this.name].push) {
                        o[this.name] = [o[this.name]];
                    }
                    o[this.name].push(this.value || '');
                } else {
                    o[this.name] = this.value || '';
                }
            });
            return o;
        };

        function Clicked(isClickedTatico, isClickedAcao) {
            isTaticoClicked = isClickedTatico;
            isNovaAcao = isClickedAcao;
        }

        /*Planejamento*/
        function ModalOpcoesEstrategico(title) {//Pos Modal1

            $('#modalLindo').modal();
            $('#modalLindo').find('.modal-body').empty();

            if (title != undefined) {
                $('#Header').html(title);
            } else {
                $('#Header').html("Planejamento");
            }

            if (planejamentoCorrentId > 0) {
                $.get(urlModalOpcoesEstrategico, { id: planejamentoCorrentId }, function (r) {
                    $('#modalLindo').find('.modal-body').empty().append(r);
                    $('#NovaAcao').show();
                });

            } else {
                $('#modalLindo').find('.modal-body').empty();
            }

            /*Botões*/
            $('#modalLindo').find('.modal-footer button').hide()
            $('#Fechar').show();
            $('#BuscarPlanejamentos').show();
            $('#NovaAcao').show();


            if (!isNovaAcao) {
                $('#NovoPlanejamento').show();
            }
            /*Fim Botões*/

        }

        function NovoPlanejamento() {//Pos Modal2
            var title = $('#Header').html();
            var title2 = " > Novo"

            $('#Header').html(title + title2);

            $.get(urlNovoPlanejamento, function (r) {
                $('#modalLindo').find('.modal-body').empty().append(r);
                if (isTaticoClicked) {
                    $('.novoItem ').hide();
                } else {
                    $('.novoItem ').show();
                }
                InitDatePiker();
            });

            /*Botões*/
            $('#modalLindo').find('.modal-footer button').hide()
            $('#Fechar2').show();
            $('#Salvar').show();

            $('#Salvar').off('click').on('click', SalvarPlanejamento);

            $('#Fechar2').off('click').on('click', function () { ModalOpcoesEstrategico(title) });
            /*Fim Botões*/
        }

        function BuscarPlanejamentos() {//Pos Modal2

            var title = $('#Header').html();
            $('#Header').html("Buscar Planejamento")

            $.get(urlBuscarPlanejamentos, function (r) {
                $('#modalLindo').find('.modal-body').empty().append(r);
                $('#filtrarPlanejamento').click();
            });

            /*Botões*/
            $('#modalLindo').find('.modal-footer button').hide()
            $('#Fechar2').show();
            $('#Selecionar').show();
            $('#Selecionar').prop('disabled', true);
            $('#Selecionar').off('click');

            $('#Fechar2').off('click').on('click', function () { ModalOpcoesEstrategico(title) });
            /*Fim Botões*/
        }

        function SalvarPlanejamento() {
            var sendObj = $('#Planejamento').serializeObject();
            $.post(urlSalvarPlanejamento, sendObj, function (r) {
                //console.log(r);
                planejamentoCorrentId = parseInt(r.Id);
                ModalOpcoesEstrategico(r.Id);

                $.post(urlGetPlanejamentoAcao, {}, function (r) { GerarTabela(r) })

            });
        }/*planejamentoCorrentId*/

        function FiltrarPlanejamento() {
            $('#Selecionar').prop('disabled', true);
            $('#Selecionar').off('click');

            var filtro = {};
            filtro["Diretoria_Id"] = $('#Diretoria_Id').val();
            filtro["Gerencia_Id"] = $('#Gerencia_Id').val();
            filtro["Coordenacao_Id"] = $('#Coordenacao_Id').val();
            filtro["TemaAssunto_Id"] = $('#TemaAssunto_Id').val();
            filtro["ObjetivoGerencial_Id"] = $('#ObjetivoGerencial_Id').val();
            filtro["Indicadores_Id"] = $('#Indicadores_Id').val();
            filtro["IndicadoresDeProjeto_Id"] = $('#IndicadoresDeProjeto_Id').val();
            filtro["IndicadoresDiretriz_Id"] = $('#IndicadoresDiretriz_Id').val();

            $.post(urlFiltraPlanejamentos, filtro, function (r) {
                $('#planejamentosFiltrado').empty().append(r);
                $('#planejamentosFiltrado > select').on('change', function (e) {
                    if (e.currentTarget.selectedIndex > 0) {
                        var id = $('#planejamentosFiltrado > select').val()
                        $('#Selecionar').prop('disabled', false);
                        $('#Selecionar').on('click', function () {
                            planejamentoCorrentId = parseInt($('#planejamentosFiltrado > select').val());
                            if (!isNovaAcao) {
                                ModalOpcoesEstrategico();
                            } else {
                                ModalAcao();
                            }
                        });
                    }
                });
            });

            /*Interação com planejamentoCorrentId*/
        }/*planejamentoCorrentId*/
        /*Fim Planejamento*/

        /*Acao*/
        function ModalAcao(isOpenModal, isFukingNewClicked) {

            if (!(planejamentoCorrentId > 0)) {
                BuscarPlanejamentos();
                return;
            } else {
                if (!$('#body > table').length) {
                    ModalOpcoesEstrategico('Planejamento Operacional');
                }
            }

            if (isFukingNewClicked) {

                if (isOpenModal)
                    $('#modalLindo').modal();

                $('#Header').html("Nova Ação");
                //$('#modalLindo').find('.modal-body').empty();

                $.get(urlNovaAcao, function (r) {

                    $('#modalLindo').find('.modal-body').append('<hr>');
                    $('#modalLindo').find('.modal-body').append(r);
                    $('#CausaMedidasXAcao_GrupoCausa_Id').empty().attr('disabled', true);
                    $('#CausaMedidasXAcao_ContramedidaGenerica_Id').empty().attr('disabled', true);

                    myfunction();

                    $('#new').click();
                    if (isFukingNewClicked) {
                        $('#new').show();
                    } else {
                        $('#new').hide();
                    }

                });

                /*Botões*/
                $('#modalLindo').find('.modal-footer button').hide()
                $('#Fechar').show();
                $('#Salvar').show();
                $('#Salvar').off('click').on('click', SalvarAcao);
                /*Fim Botões*/
            }
        }

        function EditarAcao(html, data) {
            $('#modalLindo').modal();
            $('#Header').html("Editar Ação");
            $('#modalLindo').find('.modal-body').empty().append(html);
            myfunction();
            $('#CausaMedidasXAcao_CausaGenerica_Id').val(data.Acao.CausaMedidasXAcao.CausaGenerica_Id).change();
            $('#CausaMedidasXAcao_CausaEspecifica').val(data.Acao.CausaMedidasXAcao.CausaEspecifica);
            $('#CausaMedidasXAcao_ContramedidaEspecifica').val(data.Acao.CausaMedidasXAcao.ContramedidaEspecifica);
            $('#Status').val(data.Acao.Status);
            
        }
        var model = {};

        function myfunction() {
            $('[id=CausaMedidasXAcao_CausaGenerica_Id]').off('change').on('change', function () {
                var valor = $(this).val();
                var form = $(this).parents('form');
                if (valor) {
                    $.get(GETGrupoCausa, { id: valor }, function (r) {
                        form.find('#GrupoCausa').empty().html(r);
                    });
                    $.get(GETContramedidaGenerica, { id: valor }, function (rr) {
                        form.find('#ContramedidaGenerica').empty().html(rr);
                    });
                }
                else {
                    form.find('#CausaMedidasXAcao_GrupoCausa_Id').empty().attr('disabled', true);
                    form.find('#CausaMedidasXAcao_ContramedidaGenerica_Id').empty().attr('disabled', true);
                }
            });
        }

        function SalvarAcao() {

            $('.Acao').not(':first').each(function (c, o) {

                var sendObj = $(o).serializeObject();
                sendObj['Panejamento_Id'] = planejamentoCorrentId;
                sendObj['AcaoXQuem'] = [];

                $('.quemClone').each(function (c, o) {
                    if ($(o).find('select').val())
                        sendObj['AcaoXQuem'].push({ Quem_Id: $(o).find('select').val() });
                });

                $.post(urlSalvarAcao, sendObj, function (r) {
                    $.post(urlGetPlanejamentoAcao, {}, function (r) {
                        GerarTabela(r);
                    });
                });
            });

        }

        function RemoveAcao(btn) {
            $(btn).parents('#obj').remove();
        }

        /*Fim Acao*/

        function Adicionar(btn) {

            var $tr = $(btn).closest('.tr_clone');
            var $clone;
            //if ($($tr).is("tr")) {

            $clone = $tr.clone();

            //} else if ($($tr).is("td")) {

            //    var quantidade = parseInt($tr.attr('rowspan')) - 1;
            //    $clone = $($tr).parents('tr').nextAll("*:lt(" + quantidade + ")")

            //}

            $clone.find(':text').val('');
            $tr.find('#remove').show();
            $tr.find('#adicionar').hide();
            $tr.after($clone);
        }

        function Remover(btn) {
            $(btn).parents('tr').remove();
        }

        /*Novo item*/
        var tempInput;
        var tempName;
        function Novo(btn) {
            tempInput = $(btn).parents('tr').find('input , select');
            tempName = $(btn).parents('tr').find('input , select').attr('name');
            var label = $(btn).parents('tr').find('label').html();
            $('#newItem').val("");
            $('#modalNovo').modal();
            $('#novoHeader').html("Incluir novo: " + label);
        }

        function SalvarNovo() {
            $.get(urlSalvarNovoItem + "/" + $('#newItem').val() + "/" + tempName.split('_Id')[0], {}, function (r) {
                $(tempInput).append('<option value=' + r + '> ' + $('#newItem').val() + '</option>');
                $(tempInput).val(r).change();
                $('#modalNovo').modal('hide');
                tempInput = undefined;
                tempName = undefined;
            });
        }
        /*Fim Novo item*/

        $(document).ajaxStart(function () {
            $.LoadingOverlay("show");
        });

        $(document).ajaxStop(function () {
            $.LoadingOverlay("hide");
        });

        /*Table*/
        $(document).ready(function () {
            $.post(urlGetPlanejamentoAcao, {}, function (r) { GerarTabela(r) })
        });

