            $('#modalLindo').find('.modal-body').empty().append(html);
            $('#modalLindo').find('.modal-footer button').hide();
            myfunction();

            $('#CausaMedidasXAcao_CausaGenerica_Id').val(acao.CausaMedidasXAcao.CausaGenerica_Id).change();
            $('[name="_QuandoInicio"]').val(acao._QuandoInicio.split(" ")[0]);
            $('[name="_QuandoFim"]').val(acao._QuandoFim.split(" ")[0]);
            $('#Status').val(acao.Status);
            MoneyMask();
            $('#_QuantoCusta').val(acao.QuantoCusta);
            $('#_QuantoCusta').trigger('mask.maskMoney');
            $('#_QuantoCusta').trigger('blur');

            acao.AcaoXQuem.forEach(function (o, c) {
                $($('[id=AcaoXQuemDdl]')[c]).val(o.Quem_Id).change();
                $($('[id=AcaoXQuemDdlId]')[c]).val(o.Id).change();
                if (acao._QuemObj.length > 1 && acao._QuemObj.length - 1 != c)
                    $($('[id=adicionar]')[c]).click();
            });

            $('#Edit').show();
            InitDatePiker();
        }

        function myfunction() {
            $('[id=CausaMedidasXAcao_CausaGenerica_Id]').off('change').on('change', function () {
                var valor = $(this).val();
                var form = $(this).parents('form');
                if (valor) {
                    $.get(GETGrupoCausa, { id: valor }, function (r) {
                        form.find('#GrupoCausa').empty().html(r);
                    });
                    $.get(GETContramedidaGenerica, { id: valor }, function (rr) {
                        form.find('#ContramedidaGenerica').empty().html(rr);
                    });
                }
                else {
                    form.find('#CausaMedidasXAcao_GrupoCausa_Id').empty().attr('disabled', true);
                    form.find('#CausaMedidasXAcao_ContramedidaGenerica_Id').empty().attr('disabled', true);
                }
            });
