@model Dominio.ParLevel1XModule

@{
    ViewBag.Title = "Details";
}

<div class="page-content-wrapper">
    <!-- BEGIN CONTENT BODY -->
    <div class="page-content">
        <!-- BEGIN PAGE HEADER-->
        <!-- BEGIN PAGE BAR -->
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li>
                    <a href="@Url.Action("Index", "Home")">Home</a>
                    <i class="fa fa-angle-right"></i>
                    <a href="@Url.Action("Index", "ParLevel1xModule")">@Resources.Resource.parlevel1xmodules</a>
                    <i class="fa fa-angle-right"></i>
                </li>
                <li>
                    <span>@Resources.Resource.details</span>
                </li>
            </ul>
            <div class="page-toolbar">
                <!--<div id="dashboard-report-range" class="pull-right tooltips btn btn-fit-height green" data-placement="top" data-original-title="Change dashboard date range">-->
                @*<div id="dashboard-report-range" class="pull-right tooltips btn btn-fit-height green">
                        <i class="fa fa-calendar"></i>&nbsp;
                        <span class="thin uppercase" id="currentDate"></span>&nbsp;
                    </div>*@
            </div>
        </div>
        <!-- END PAGE BAR -->
        <!-- END PAGE HEADER-->
        <div class="row">
            <div class="col-xs-6">
                <div class="portlet light bordered">
                    <div class="portlet-title tabbable-line">
                        <div class="caption">
                            <i class="icon-share font-dark"></i>
                            <span class="caption-subject font-dark bold uppercase">@Resources.Resource.details @Resources.Resource.module</span>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <div class="tab-content">
                            <div class="tab-pane active" id="setup_tab">

                                <div>
                                    <dl class="dl-horizontal">
                                        <dt>
                                            @Html.DisplayName(Resources.Resource.parlevel1)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.ParLevel1.Name)
                                        </dd>

                                        <dt>
                                            @Html.DisplayName(Resources.Resource.parModule)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.ParModule.Name)
                                        </dd>

                                        @*<dt>
                                            @Html.DisplayName(Resources.Resource.points)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Points)
                                        </dd>*@

                                        <dt>
                                            @Html.DisplayName(Resources.Resource.is_active)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.IsActive)
                                        </dd>

                                        <dt>
                                            @Html.DisplayName(Resources.Resource.effective_date_start)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.EffectiveDateStart)
                                        </dd>

                                        @*<dt>
                                            @Html.DisplayName(Resources.Resource.effective_date_end)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.EffectiveDateEnd)
                                        </dd>*@
                                    </dl>
                                </div>
                                <p>
                                    @Html.ActionLink(Resources.Resource.edit, "Edit", new { id = Model.Id }, new { @class = "btn btn-success" })
                                    @Html.ActionLink(Resources.Resource.back_to_list, "Index", null, new { @class = "btn btn-default" })
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xs-6">
                @*@Html.Partial("~/Views/ParModuleXModules/Index.cshtml", Model.ParModuleXModule1)*@
            </div>
        </div>
    </div>
    <!-- END CONTENT BODY -->
</div>


<script>

    $(".sidebar-toggler").removeClass("hide");
    $(".page-sidebar-wrapper").removeClass("hide");
    navbarSelect("navbar3");

</script>

<script>
    $(document).ready(function () {
        closeLeftSidebar();
    });
</script>

