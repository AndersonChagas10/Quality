    let scrollToForm = function () {
        document.body.scrollTop = 0;
        document.documentElement.scrollTop = 330;
    }

    let setSelectLevel1 = function (data) {
        preencheSelect('selectL1', 'Name', 'Id', data.ParLevels1, 'Criar novo indicador', preencheL1);
        preencheSelect('selectL2AvamParLevel1', 'Name', 'Id', data.ParLevels1, 'Selecione o Indicador');
        preencheSelect('selectL3AvamParLevel1', 'Name', 'Id', data.ParLevels1, 'Selecione o Indicador');
        preencheSelect('selectL3PesoParLevel1', 'Name', 'Id', data.ParLevels1, 'Selecione o Indicador');
        preencheSelect('selectL3TipoDadoParLevel1', 'Name', 'Id', data.ParLevels1, 'Selecione o Indicador');
        getSelectsL1();
    }

    let setSelectLevel2 = function (data) {
        preencheSelect('selectL2', 'Name', 'Id', data.ParLevels2, 'Criar novo monitoramento', preencheL2);
        preencheSelect('selectL3AvamParLevel2', 'Name', 'Id', data.ParLevels2, 'Selecione o Monitoramento');
        preencheSelect('selectL3PesoParLevel2', 'Name', 'Id', data.ParLevels2, 'Selecione o Monitoramento');
        preencheSelect('selectL3TipoDadoParLevel2', 'Name', 'Id', data.ParLevels2, 'Selecione o Monitoramento');
        //getSelectsL2();
    }

    let setSelectLevel3 = function (data) {
        preencheSelect('selectL3', 'Name', 'Id', data.ParLevels3, 'Criar nova tarefa', preencheL3);
        //getSelectsL3();
    }

    $(document).ready(function () {

        getGenericAjax('@Url.Action("Get", "api/ParLevel1Api")', 'Get', setSelectLevel1);

        getGenericAjax('@Url.Action("Get", "api/ParLevel2Api")', 'Get', setSelectLevel2);

        getGenericAjax('@Url.Action("Get", "api/ParLevel3Api")', 'Get', setSelectLevel3);

        verifyTabs('selectL1', 'l1');
        verifyTabs('selectL2', 'l2');
        verifyTabs('selectL3', 'l3');

    });

    let preencheSelect = function (id, name, value, list, defaultText, callBack) {

        $(`select#${id}`).empty();
        $(`select#${id}`).append(`<option value="0" selected>${defaultText}</option>`);

        if (list && list.length > 0) {

            let listVinculados = $.grep(list, (item) => { return (item.GroupName != "" && item.GroupName != undefined) });
            let listNaoVinculados = $.grep(list, (item) => { return (item.GroupName == undefined || item.GroupName == "") });

            if (listVinculados && listVinculados.length > 0) {

                [... new Set(listVinculados.map(x => x.GroupName))].forEach((groupName) => {

                    $(`select#${id}`).append('<optgroup label="' + groupName + '">');

                    listVinculados.filter(x => x.GroupName == groupName).forEach((o) => {

                        if (o.Hash != undefined)
                            $(`select#${id}`).append(`<option value="${o[value]}">${o.Hash} / ${o[value]} - ${o[name]}</option>`);
                        else
                            $(`select#${id}`).append(`<option value="${o[value]}">${o[value]} - ${o[name]}</option>`);

                    });

                    $(`select#${id}`).append('</optgroup>');

                });
            }

            $(listNaoVinculados).each(function (i, o) {
                if (o.Hash != undefined)
                    $(`select#${id}`).append(`<option value="${o[value]}">${o.Hash} / ${o[value]} - ${o[name]}</option>`);
                else
                    $(`select#${id}`).append(`<option value="${o[value]}">${o[value]} - ${o[name]}</option>`);
            });

        }

        if (callBack) {
            $(`select#${id}`).off('change').on('change', callBack);
        }

        $(`select#${id}`).select2();
    }

    let listaTipoDadoDeEntradaVinculadosaEmpresaSelecionada = function (listaTipoDado) {
        if (listaTipoDado != null) {
            if (parseInt($("#selectParCompanyGeral :selected").val()) != 0) {

                var listaVinculadaAUnidade = jQuery.grep(listaTipoDado, function (n, i) {
                    return (n.ParCompany_Id === parseInt($("#selectParCompanyGeral :selected").val()) || n.ParCompany_Id == null);
                });

                ParL3TipoDado.load(listaVinculadaAUnidade);

            } else {
                ParL3TipoDado.load(listaTipoDado);
            }
        }
    }

    let listaVinculoPesoVinculadosaEmpresaSelecionada = function (lista) {
        if (lista != null) {

            if (parseInt($("#selectParCompanyGeral :selected").val()) != 0) {

                var listaPesoVinculadaAUnidade = jQuery.grep(lista, function (n, i) {
                    return (n.ParCompany_Id === parseInt($("#selectParCompanyGeral :selected").val()) || n.ParCompany_Id == null);
                });

                ParL3Peso.load(listaPesoVinculadaAUnidade);

            } else {
                ParL3Peso.load(lista);
            }
        }
    }

    let listaAvaliacaoAmostraVinculadosaEmpresaSelecionada = function (lista) {
        if (lista != null) {

            if (parseInt($("#selectParCompanyGeral :selected").val()) != 0) {

                var listaAvAmVinculadaAUnidade = jQuery.grep(lista, function (n, i) {
                    return (n.ParCompany_Id === parseInt($("#selectParCompanyGeral :selected").val()) || n.ParCompany_Id == null);
                });

                parDCAvam.load(listaAvAmVinculadaAUnidade);

            } else {
                parDCAvam.load(lista);
            }
        }
    }


    let MontaSelectDepartamentoVinculadoAUnidadeSelecionada = function (selectId, empresaSelecionada, mensagem) {

        var unidadeSelecionada;
        if (empresaSelecionada == null) {
             unidadeSelecionada = parseInt($("#selectParCompanyGeral :selected").val());
        } else {
            unidadeSelecionada = empresaSelecionada
        }

        //var mensagem;
        if (mensagem == null) {

            if (selectId == 'selectL3PesoParDepartment') {
                mensagem = 'Todos';
            } else {
                mensagem = 'Selecione o centro de custo';
            }
        }

        if (unidadeSelecionada == 0) {

            ParDepartmentsGlobal = [];
            preencheSelect(selectId, 'Name', 'Id', ParDepartmentsGlobal, mensagem);
        } else {

            var departamentosVinculados = jQuery.grep(ParDepartmentsGlobal, function (n, i) {
                return (n.ParCompany_Id === unidadeSelecionada);
            });

            preencheSelect(selectId, 'Name', 'Id', departamentosVinculados, mensagem);
        }

        $('#' + selectId).trigger("change");
    }

    let GetParDepartmentsByUnit = function (unit) {
        
        if (!unit) {
            MontaSelectDepartamentoVinculadoAUnidadeSelecionada('selectDCDepartment', null, null);
            MontaSelectDepartamentoVinculadoAUnidadeSelecionada('selectL2GeralParDepartment', null, null);
            MontaSelectDepartamentoVinculadoAUnidadeSelecionada('selectL3PesoParDepartment', null, null);

            MontaSelectDepartamentoVinculadoAUnidadeSelecionada('selectL3TipoDadoParDepartment', null, null);

            disableEnableAbasSemFiltroGeral();
            return;
        }

        openLoader(Resources("please_wait"));

        $.ajax({

            url: `@Url.Action("GetParCargoXDepartmentByUnit", "api/ParLevel1Api")/${unit}`,
            type: 'Get',
            dataType: "json",
            data: {},
            contentType: "application/json",
            success: function (data) {
                
                ParDepartmentsGlobal = data.ParDepartments;
                ParCargosGlobal = data.ParCargos;
                ParCargoXDepartmentsGlobal = data.ParCargoXDepartments

                MontaSelectDepartamentoVinculadoAUnidadeSelecionada('selectDCDepartment', unit, null);
                MontaSelectDepartamentoVinculadoAUnidadeSelecionada('selectL2GeralParDepartment', unit, null);
                MontaSelectDepartamentoVinculadoAUnidadeSelecionada('selectL3PesoParDepartment', unit, null);

                MontaSelectDepartamentoVinculadoAUnidadeSelecionada('selectL3TipoDadoParDepartment', unit, null);

                preencheSelect('selectL3ParCargo', 'Name', 'Id', ParCargosGlobal, 'Todos');

                disableEnableAbasSemFiltroGeral();

                closeLoader();
            },
            error: function (request, error) {
                closeLoader();
                openMessageModal("Ocorreu um erro", JSON.stringify(request));
                console.log(error)
            }
        });

    }

    let MontaSelectUnidades = function () {

        var unidadeSelecionadaTeste = parseInt($("#selectParCompanyGeral :selected").val());

        if (unidadeSelecionadaTeste == 0) {
            preencheSelect('selectDCAvamParCompany', 'Name', 'Id', parCompanysGlobal, "Todos");
            preencheSelect('selectL3TipoDadoParCompany', 'Name', 'Id', parCompanysGlobal, "Todos");
            preencheSelect('selectL3PesoParCompany', 'Name', 'Id', parCompanysGlobal, "Todos");
