@model Dominio.ParReportXFilter

<div class="page-content-wrapper">
    <div class="page-content">
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li>
                    <a href="@Url.Action("Index", "Home")">Home</a>
                    <i class="fa fa-angle-right"></i>
                    <a href="@Url.Action("Details", "ReportXUserSgq",new { id = ViewBag.ReportXUser_Id })">Cadastro relatório X Filtro</a>
                    <i class="fa fa-angle-right"></i>
                </li>
                <li>
                    <span>@Resources.Resource.delete</span>
                </li>
            </ul>
            <div class="page-toolbar"></div>
        </div>
        <div class="row">
            <div class="col-xs-12">
                <div class="portlet light bordered">
                    <div class="portlet-title tabbable-line">
                        <div class="caption">
                            <i class="icon-share font-dark"></i>
                            <span class="caption-subject font-dark bold uppercase">Cadastro relatório X Filtro</span>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <div class="tab-content">
                            <div class="tab-pane active" id="setup_tab">
                                <h3>@Resources.Resource.confirm_delete?</h3>
                                <div>
                                    <dl class="dl-horizontal">

                                        <dt>
                                            Nome
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.FilterLevel_Name)
                                        </dd>

                                        <dt>
                                           Esta Ativo:
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.IsActive)
                                        </dd>


                                    </dl>

                                    @using (Html.BeginForm())
                                    {
                                        @Html.AntiForgeryToken()

                                    <div class="form-actions no-color">
                                        <input type="submit" value="Excluir" class="btn btn-danger" />
                                        @Html.ActionLink(Resources.Resource.back as string + " para detalhes", "Details", "ReportXUserSgq", new { id = ViewBag.ReportXUser_Id }, new { @class = "btn btn-default" }) 
                                    </div>
                                    }

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
