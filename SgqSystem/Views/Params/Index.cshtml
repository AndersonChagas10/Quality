@model SgqSystem.ViewModels.ParamsViewModel
@{
    ViewBag.Title = "Parametrization";
    var url = Url.Action("AddLevel01", "api/Params"); //URL relativa
}

<h2>@ViewBag.Title</h2>

@using (Html.BeginForm())
{
    @Html.LabelFor(m => m.Name, htmlAttributes: new { @class = "", @style = "width:210px" })
    @Html.TextBoxFor(m => m.Name, htmlAttributes: new { @class = "form-control", @style = "width:210px" })
    @Html.TextBoxFor(m => m.exemplo.teste, htmlAttributes: new { @class = "form-control", @style = "width:210px" })
    @Html.DropDownList("_ShiftSelectListExample", Model.ShiftSelectListExample, "Select one", htmlAttributes: new { @class = "form-control", @style = "width:210px" })
    <input type="button" value="Enviar Para API" onclick="indexParamsLevel1.enviarDadosParaApi()"/>
}

<script>

    var url1 = '@Html.Raw(@url)';

    var indexParamsLevel1 = {

        enviarDadosParaApi: function (e) {

            var dados = $('form').serializeObject();

            $.post(url1, dados, function (r, b, x) {
                console.log(b);
                console.log(x);
                console.log(r);
            });

        }

    }

</script>