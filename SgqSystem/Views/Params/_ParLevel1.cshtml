            $('#counterEditHidden').append(objHiddenCounter);
        },
        RemoveCounter: function (e, c) {
            e.parents('tr').remove();
            Counter.ObjCounters.splice(c, 1);
        },
        RemoveCounterEdicao: function (e, id) {
            e.parents('tr').hide();

            Counter.ObjCounterRemove.push({ Id: id })


        },

    }


