                            valueDecimals: 2,
                            shared: true,
                            format: '{point.y:.2f}',
                            format: '{point:.2f}',
                        },
                        plotOptions: {
                            column: {
                                grouping: false,
                                shadow: false,
                                borderWidth: 0
                            }
                        },
                        series: [{
                            visible: false,
                            name: 'NC',
                            color: 'rgba(77,143,189,1)',
                            data: nc,
                            yAxis: 0,
                            type: 'line',
                            marker: {
                                enabled: false
                            },
                            pointPadding: 0.2,
                            //point: {
                            //    events: {
                            //        click: function () {
                            //            clickFiltroData(this);
                            //        }
                            //    }
                            //},
                            dataLabels: {
                                enabled: false,
                                color: 'black',
                                align: 'center',
                                format: '{point.y:.2f}',
                                style: {
