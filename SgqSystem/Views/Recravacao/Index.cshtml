                                            2	Número de defeitos ok 
                                            3	Intervalos ok
                                            4	Calculado
                                            5	Texto
                                            99	NOVO CALCULADO POR OUTRO*@

                                            @*3	Intervalos*@
                                            <span ng-show="(ParLevel3.ParLevel3Value[0] && ParLevel3.ParLevel3Value[0].ParLevel3InputType_Id == '3')">
                                                <input title="Valores aceitáveis entre {{ParLevel3.ParLevel3Value[0].IntervalMin}} e {{ParLevel3.ParLevel3Value[0].IntervalMax}}"
                                                       ng-model="ParLevel3.ResultValue['Ponto' + ($index +1)]"
                                                       style="width:60%"
                                                       type="number" />
                                                <i ng-show="!(ParLevel3.ParLevel3Value[0].IntervalMin <= ParLevel3.ResultValue['Ponto' + ($index +1)] && ParLevel3.ResultValue['Ponto' + ($index +1)] <= ParLevel3.ParLevel3Value[0].IntervalMax)" class="fa fa-times" aria-hidden="true"></i>
                                                <i ng-show="(ParLevel3.ParLevel3Value[0].IntervalMin <= ParLevel3.ResultValue['Ponto' + ($index +1)] && ParLevel3.ResultValue['Ponto' + ($index +1)] <= ParLevel3.ParLevel3Value[0].IntervalMax)" class="fa fa-check" aria-hidden="true"></i>
                                            </span>

                                            @*1	Binário*@
                                            <span ng-show="(ParLevel3.ParLevel3Value[0] && ParLevel3.ParLevel3Value[0].ParLevel3InputType_Id == '1')">
                                                <input type="radio" ng-model="etc" ng-value="true" ng-change="ParLevel3.ResultValue['Ponto' + ($index +1)] = 0" /> {{ParLevel3.ParLevel3Value[0].ParLevel3BoolTrue.Name}}
                                                <br />
                                                <input type="radio" ng-model="etc" ng-value="false" ng-change="ParLevel3.ResultValue['Ponto' + ($index +1)] = 1" /> {{ParLevel3.ParLevel3Value[0].ParLevel3BoolFalse.Name}}
                                            </span>

                                            @*4	Calculado*@
                                            @*<span ng-show="(ParLevel3.ParLevel3Value[0] && ParLevel3.ParLevel3Value[0].ParLevel3InputType_Id == '4')">
                                                CALCULADO
                                            </span>*@

                                            @*2	Número de defeitos*@
                                            <span ng-show="(ParLevel3.ParLevel3Value[0] && ParLevel3.ParLevel3Value[0].ParLevel3InputType_Id == '2')">
                                                <input title="Valores aceitáveis entre {{ParLevel3.ParLevel3Value[0].IntervalMin}} e {{ParLevel3.ParLevel3Value[0].IntervalMax}}"
                                                       ng-model="ParLevel3.ResultValue[('Ponto' + ($index +1))]"
                                                       style="width:60%"
                                                       type="number" />
                                                <i ng-show="!(ParLevel3.ParLevel3Value[0].IntervalMin <= ParLevel3.ResultValue['Ponto' + ($index +1)] && ParLevel3.ResultValue['Ponto' + ($index +1)] <= ParLevel3.ParLevel3Value[0].IntervalMax)" class="fa fa-times" aria-hidden="true"></i>
                                                <i ng-show="(ParLevel3.ParLevel3Value[0].IntervalMin <= ParLevel3.ResultValue['Ponto' + ($index +1)] && ParLevel3.ResultValue['Ponto' + ($index +1)] <= ParLevel3.ParLevel3Value[0].IntervalMax)" class="fa fa-check" aria-hidden="true"></i>
                                            </span>

                                            @*5	Texto*@
                                            @*<span ng-show="(ParLevel3.ParLevel3Value[0] && ParLevel3.ParLevel3Value[0].ParLevel3InputType_Id == '5')">
                                                Outro: <input type="text" />
                                            </span>*@    

                                            @*99 NOVO CALCULADO POR OUTRO*@
                                            @*<span ng-show="(ParLevel3.ParLevel3Value[0] && ParLevel3.ParLevel3Value[0].ParLevel3InputType_Id == '99')">
                                                NOVO CALCULADO POR OUTRO
                                            </span>*@
                                        </td>

                                        <td>@*{{ParLevel3}}*@</td>
                                    </tr>

                                </tbody>
                            </table>

                        </div>
                        @*{{item}}*@    
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                            <button type="button" class="btn btn-primary">Save changes</button>
                        </div>
                    </div>
                </div>
            </div>

        </div>

        <button ng-show="seletedLinha.Id > 0" type="button" ng-click="newLata(seletedLinha)" class="btn btn-sucsdfcess btn-lg">
            <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>Adicionar Lata
        </button>

    </div>



    <button type="button" ng-click="saveState()" class="btn btn-sucsdfcess btn-lg">
        <span class="glyphicon glyphicon-save-file" aria-hidden="true"></span>POST LINHA
    </button>
    <button type="button" ng-click="getState()" class="btn btn-sucsdfcess btn-lg">
        <span class="glyphicon glyphicon-save-file" aria-hidden="true"></span>GET TODAS AS LINHAS
    </button>
    <button type="button" ng-click="getLinhas()" class="btn btn-sucsdfcess btn-lg">
        <span class="glyphicon glyphicon-save-file" aria-hidden="true"></span>GET LINHAS DB
    </button>
    <button type="button" ng-click="mockLinhas()" class="btn btn-sucsdfcess btn-lg">
        <span class="glyphicon glyphicon-save-file" aria-hidden="true"></span>MOCK
    </button>




    <hr />


    @*{{linhas[0].ListParlevel3}}*@
    <hr />
    @*{{linhas}}*@
    <hr />
