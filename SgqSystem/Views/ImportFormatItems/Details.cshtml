@model Dominio.ImportFormatItem

@{
    ViewBag.Title = Resources.Resource.details;
}



<div class="page-content-wrapper">
    <!-- BEGIN CONTENT BODY -->
    <div class="page-content">
        <!-- BEGIN PAGE HEADER-->
        <!-- BEGIN PAGE BAR -->
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li>
                    <a href="@Url.Action("Index", "Home")">Home</a>
                    <i class="fa fa-angle-right"></i>
                    <a href="@Url.Action("Index", "ImportFormatItems")">@Resources.Resource.details</a>
                    <i class="fa fa-angle-right"></i>
                </li>
                <li>
                    <span>@Resources.Resource.details @Resources.Resource.import_format_itens</span>
                </li>
            </ul>
            <div class="page-toolbar">
                <!--<div id="dashboard-report-range" class="pull-right tooltips btn btn-fit-height green" data-placement="top" data-original-title="Change dashboard date range">-->
                @*<div id="dashboard-report-range" class="pull-right tooltips btn btn-fit-height green">
                        <i class="fa fa-calendar"></i>&nbsp;
                        <span class="thin uppercase" id="currentDate"></span>&nbsp;
                    </div>*@
            </div>
        </div>
        <!-- END PAGE BAR -->
        <!-- END PAGE HEADER-->
        <div class="row">
            <div class="col-xs-6">
                <div class="portlet light bordered">
                    <div class="portlet-title tabbable-line">
                        <div class="caption">
                            <i class="icon-share font-dark"></i>
                            <span class="caption-subject font-dark bold uppercase">@Resources.Resource.details</span>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <div class="tab-content">
                            <div class="tab-pane active" id="setup_tab">

                                <div>
                                    <dl class="dl-horizontal">
                                        <dt>
                                            @Html.DisplayName(Resources.Resource.key)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Key)
                                        </dd>

                                        <dt>
                                            @Html.DisplayName(Resources.Resource.value)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Value)
                                        </dd>

                                        <dt>
                                            @Html.DisplayName(Resources.Resource.add_date)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.AddDate)
                                        </dd>

                                        <dt>
                                            @Html.DisplayName(Resources.Resource.alter_date)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.AlterDate)
                                        </dd>

                                        <dt>
                                            @Html.DisplayName(Resources.Resource.import_format_id)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.ImportFormat_Id)
                                        </dd>

                                    </dl>
                                </div>
                                <p>
                                    @Html.ActionLink(Resources.Resource.edit, "Edit", new { id = Model.Id }, new { @class = "btn btn-success" })
                                    @Html.ActionLink(Resources.Resource.back_to_list, "Index", null, new { @class = "btn btn-default" })
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            @*<div class="col-xs-6">
                    @Html.Partial("~/Views/ParModuleXModules/Index.cshtml", Model.ImportFormatItems)
                </div>*@
        </div>
    </div>
    <!-- END CONTENT BODY -->
</div>



