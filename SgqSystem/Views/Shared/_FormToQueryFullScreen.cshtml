        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">

                    <p style="margin-left: -30px;">
                        <button type="button" class="btn btn-primary pull-right refresh-btn-right-top @*hide*@" id="btnSend" onclick="Send();">@Resources.Resource.refresh</button>

                        <button type="button" class="btn btn-primary btn-xs" id="menu-toggle"><i class="glyphicon glyphicon-chevron-left"></i></button>
