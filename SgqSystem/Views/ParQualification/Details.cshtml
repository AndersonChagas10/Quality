@model Dominio.ParQualification

@{
    ViewBag.Title = "Detalhes - Qualificação";
    ViewBag.ParQualification_Id = Model.Id;
}

<div class="page-content-wrapper">
    <div class="page-content">
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li>
                    <a href="@Url.Action("Index", "Home")">Home</a>
                    <i class="fa fa-angle-right"></i>
                    <a href="@Url.Action("Index", "ParQualification")">Qualificação</a>
                    <i class="fa fa-angle-right"></i>
                </li>
                <li>
                    <span>@Resources.Resource.details </span>
                </li>
            </ul>
            <div class="page-toolbar">
            </div>
        </div>
        <div class="row">
            <div class="col-xs-4">
                <div class="portlet light bordered">
                    <div class="portlet-title tabbable-line">
                        <div class="caption">
                            <i class="icon-share font-dark"></i>
                            <span class="caption-subject font-dark bold uppercase">@Resources.Resource.details Qualificação</span>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <div class="tab-content">
                            <div class="tab-pane active" id="setup_tab">

                                <div>
                                    <dl class="dl-horizontal">
                                        <dt>
                                            @Resources.Resource.name
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Name)
                                        </dd>

                                        <dt>
                                            @Resources.Resource.is_active
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.IsActive)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.AddDate)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.AddDate)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.AlterDate)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.AlterDate)
                                        </dd>

                                    </dl>
                                </div>
                                <p>
                                    @Html.ActionLink(Resources.Resource.edit as string, "Edit", new { id = Model.Id }, new { @class = "btn btn-success" })
                                    @Html.ActionLink(Resources.Resource.back_to_list as string, "Index", null, new { @class = "btn btn-default" })
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xs-8">
            </div>
        </div>
    </div>
</div>


<script>

    $(".sidebar-toggler").removeClass("hide");
    $(".page-sidebar-wrapper").removeClass("hide");
    navbarSelect("navbar4");

</script>

<script>
    $(document).ready(function () {
        closeLeftSidebar();
    });
</script>