@model Dominio.ReportXUserSgq

<div class="page-content-wrapper">
    <div class="page-content">
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li>
                    <a href="@Url.Action("Index", "Home")">Home</a>
                    <i class="fa fa-angle-right"></i>
                    <a href="@Url.Action("Index", "ReportXUserSgq")">@Resources.Resource.report_files</a>
                    <i class="fa fa-angle-right"></i>
                </li>
                <li>
                    <span>@Resources.Resource.delete</span>
                </li>
            </ul>
            <div class="page-toolbar"></div>
        </div>
        <div class="row">
            <div class="col-xs-12">
                <div class="portlet light bordered">
                    <div class="portlet-title tabbable-line">
                        <div class="caption">
                            <i class="icon-share font-dark"></i>
                            <span class="caption-subject font-dark bold uppercase">@Resources.Resource.report_files</span>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <div class="tab-content">
                            <div class="tab-pane active" id="setup_tab">
                                <h3>@Resources.Resource.confirm_delete?</h3>
                                <div>
                                    <dl class="dl-horizontal">
                                        <dt>
                                            @Html.DisplayNameFor(model => model.Aprovador)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Aprovador)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.Elaborador)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Elaborador)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.ItemMenu_Id)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.ItemMenu.Name)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.ParCompany_Id)
                                        </dt>

                                        <dd>
                                            @if (Model.ParCompany_Id == null)
                                            {
                                                @Resources.Resource.all;
                                            }
                                            else
                                            {
                                                @Html.DisplayFor(model => model.ParCompany.Name)
                                            }
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.ParLevel1_Id)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.ParLevel1.Name)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.NomeRelatorio)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.NomeRelatorio)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.CodigoRelatorio)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.CodigoRelatorio)
                                        </dd>

                                    </dl>

                                    @using (Html.BeginForm())
                                    {
                                        @Html.AntiForgeryToken()

                                        <div class="form-actions no-color">
                                            <input type="submit" value="Excluir" class="btn btn-danger" /> 
                                            @Html.ActionLink("Voltar", "Index", null, new { @class = "btn btn-default" })
                                        </div>
                                    }

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
